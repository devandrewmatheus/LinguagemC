#include <stdio.h>

int main() {

    int N,i,x, soma;
        printf("QUANTOS NUMEROS V√ÉO SER DIGITADOS? ");
    scanf("%d", &N);

    soma = 0;

    for (i = 1; i <= N; i++) {

        printf("Digite um numero");
        scanf("%d", &x);
        soma = soma + x;
    }

    printf("A SOMA DEU: %d", soma);

}
